{
  "installCommand": "cd chatbotWeb/chatBotWeb && npm ci --include=dev --no-audit --no-fund",
  "buildCommand": "cd chatbotWeb/chatBotWeb && npm run build",
  "outputDirectory": "chatbotWeb/chatBotWeb/dist",
  "rewrites": [
    {
      "source": "/(chat|rag|transcript|auth)(.*)",
      "destination": "/api/index.py"
    }
  ]
}
